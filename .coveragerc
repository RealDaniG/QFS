[run]
branch = True
parallel = True
source =
    v13
    v13/ATLAS/src
omit =
    */__pycache__/*
    */node_modules/*
    */.next/*
    */coverage/*
    */dist/*
    */build/*
    */venv/*
    */.venv/*
    */site-packages/*
    */tests/*
    */test_*.*
    */*_test.py
    */docs/*
    */scripts/*

[report]
show_missing = True
skip_covered = False
precision = 1
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:
