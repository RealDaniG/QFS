    "dev": "next dev",
    "dev:fast": "next dev --turbo",
    "build": "npm run build:win",
    "build:win": "npm run prebuild && next build && xcopy .next\\static .next\\standalone\\.next\\static /E /I /Y && xcopy public .next\\standalone\\public /E /I /Y",
    "export:desktop": "set NEXT_PUBLIC_IS_ELECTRON_BUILD=true && next build --no-lint && if not exist desktop\\renderer mkdir desktop\\renderer && xcopy out\\* desktop\\renderer\\ /E /I /Y",
    "start": "python scripts/proxy_server.py",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "electron:build": "npm run export:desktop && cd desktop && npm install && npm run build:win"
